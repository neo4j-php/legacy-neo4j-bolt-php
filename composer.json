{
    "name": "laudis/graphaware-neo4j-bolt-legacy",
    "description": "Neo4j Bolt Binary Protocol PHP Driver",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Ghlen Nagels",
            "email": "ghlen@pm.me"
        }
    ],
    "require": {
        "php": "^7.0 || ^8.0",
        "ext-bcmath": "*",
        "ext-mbstring": "*",
        "symfony/event-dispatcher": "^2.7|^3.0|^4.0",
        "graphaware/neo4j-common": "^3.4",
        "myclabs/php-enum": "^1.4",
        "laudis/neo4j-php-client": "v2.0.x-dev",
        "stefanak-michal/bolt": "^2.3"
    },
    "autoload": {
      "psr-4": {
          "GraphAware\\Bolt\\": "src/"
      }
    },
    "autoload-dev": {
        "psr-4": {
            "GraphAware\\Bolt\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5.4",
        "symfony/stopwatch": "^2.7",
        "behat/behat": "~3.0.4"
    }
}
